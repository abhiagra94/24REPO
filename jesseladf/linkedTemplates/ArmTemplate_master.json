{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "jesseladf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_Cloud234_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Cloud234'"
		},
		"sreeniadlsgen2strg_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sreeniadlsgen2strg_LS'"
		},
		"sreeniadlsgen2strg_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sreeniadlsgen2strg.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "jesseladf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_Cloud234_connectionString": {
						"value": "[parameters('LS_Cloud234_connectionString')]"
					},
					"sreeniadlsgen2strg_LS_accountKey": {
						"value": "[parameters('sreeniadlsgen2strg_LS_accountKey')]"
					},
					"sreeniadlsgen2strg_LS_properties_typeProperties_url": {
						"value": "[parameters('sreeniadlsgen2strg_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}